{
  "name": "unplugin-lingui",
  "type": "module",
  "version": "0.0.4",
  "packageManager": "pnpm@10.15.1",
  "description": "unplugin for lingui",
  "author": "Brendan Dash <me@aiwan.run> (https://aiwan.run)",
  "license": "MIT",
  "homepage": "https://github.com/Debbl/unplugin-lingui#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Debbl/unplugin-lingui.git"
  },
  "bugs": "https://github.com/Debbl/unplugin-lingui/issues",
  "keywords": [
    "lingui",
    "unplugin",
    "vite",
    "lingui-loader",
    "esbuild",
    "rolldown",
    "rollup",
    "rspack",
    "webpack"
  ],
  "exports": {
    ".": "./dist/index.js",
    "./esbuild": "./dist/esbuild.js",
    "./rolldown": "./dist/rolldown.js",
    "./rollup": "./dist/rollup.js",
    "./rspack": "./dist/rspack.js",
    "./vite": "./dist/vite.js",
    "./webpack": "./dist/webpack.js",
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "dev": "pnpm build && node ./dist/index.cjs",
    "build": "tsdown && pnpm build:dts",
    "build:dts": "tsc",
    "test": "vitest",
    "release": "pnpm build && bumpp && pnpm publish",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "@lingui/cli": "^5.5.0",
    "@lingui/conf": "^5.5.0",
    "@lingui/format-json": "^5.5.0",
    "unplugin": "^2.3.10"
  },
  "devDependencies": {
    "@types/node": "^24.3.1",
    "bumpp": "^10.2.3",
    "eslint": "^9.35.0",
    "lint-staged": "^16.1.6",
    "simple-git-hooks": "^2.13.1",
    "tsdown": "^0.15.0",
    "typescript": "^5.9.2",
    "vite": "^7.1.5",
    "vitest": "^3.2.4",
    "webpack": "^5.101.3"
  }
}
